<?xml version="1.0"?>

<PropertyList>

	<path>egtgauge.ac</path>

  <animation>
    <type>material</type>
    <object-name>Circle</object-name>
    <object-name>Circle.001</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.6 </red>
      <green> 0.6 </green>
      <blue>  0.6 </blue>
    </emission>
  <condition>
    <property>systems/electrical/on</property>
  </condition>
  </animation>
  
 
  
<animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>engines/engine/egt-degf</property>
        <interpolation>
            <entry><ind>32</ind><dep>0</dep></entry>
            <entry><ind>1800</ind><dep>233</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
</animation>
  

</PropertyList>

