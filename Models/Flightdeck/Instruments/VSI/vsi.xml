<PropertyList>

 <path>vsi.ac</path>
  
  <animation>
    <type>material</type>
    <object-name>vsiface</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.6 </red>
      <green> 0.6 </green>
      <blue>  0.6 </blue>
    </emission>
  <condition>
    <property>systems/electrical/on</property>
  </condition>
  </animation>


  <!--variometer-->
	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
		<interpolation>
			<entry><ind>-500</ind><dep>30</dep></entry>
			<entry><ind>-1000</ind><dep>60</dep></entry>
			<entry><ind>-2000</ind><dep>100</dep></entry>
			<entry><ind>-4000</ind><dep>140</dep></entry>
			<entry><ind>-6000</ind><dep>170</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>500</ind><dep>-30</dep></entry>
			<entry><ind>1000</ind><dep>-60</dep></entry>
			<entry><ind>2000</ind><dep>-100</dep></entry>
			<entry><ind>4000</ind><dep>-145</dep></entry>
			<entry><ind>6000</ind><dep>-170</dep></entry>
		</interpolation>
  <center>
   <x-m>0.004</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
	</animation>
  

 
 </PropertyList>