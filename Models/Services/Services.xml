<!-- 777-300 George Luiz -->
<PropertyList>
  <!-- Chocks -->
  <model>
    <name>Chocks</name>
    <path>Aircraft/737-400/Models/Services/Chocks/chocks.xml</path>
    <condition>
      <and>
        <property>/controls/groundservice/chocks</property>
        <less-than>
          <property>velocities/groundspeed-kt</property>
          <value>1</value>
        </less-than>
        <greater-than>
          <property>velocities/groundspeed-kt</property>
          <value>-1</value>
        </greater-than>
      </and>
    </condition>
    <offsets>
      <x-m>0.00</x-m>
      <y-m>0.00</y-m>
      <z-m>0.00</z-m>
    </offsets>
  </model>
  <!-- Engine Safety Cones -->
  <model>
    <name>Cone</name>
    <path>Aircraft/737-400/Models/Services/Safety-Cones/safety-cones.xml</path>
    <condition>
      <and>
        <property>/controls/groundservice/safety-cones</property>
        <less-than>
          <property>velocities/groundspeed-kt</property>
          <value>1</value>
        </less-than>
        <greater-than>
          <property>velocities/groundspeed-kt</property>
          <value>-1</value>
        </greater-than>
      </and>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>
  <!-- Passenger Stairway (Front)-->
